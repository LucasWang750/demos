{
  "name": "opensearch-vscode-extension",
  "version": "1.0.0",
  "description": "OpenSearch Visual Studio Code Extension for Demos",
  "main": "./out/opensearch-extension.js",
  "author": "Kyle Davis",
  "license": "Apache-2.0",
  "engines": {
    "vscode": "^1.65.0"
  },
  "repository": "https://github.com/opensearch-project/demos",
  "contributes": {
    "commands": [
      {
        "command": "opensearch.start",
        "title": "OpenSearch Demo Control Panel"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished",
    "onWebviewPanel:opensearch",
    "onCommand:opensearch.start"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.65.0",
    "@types/vscode-webview": "^1.57.0",
    "eslint": "^8.10.0",
    "typescript": "^4.6.2",
    "vsce": "^2.7.0"
  },
  "publisher" : "opensearch-project"
}
